
SHELL = /bin/sh

# V=0 quiet, V=1 verbose.  other values don't work.
V = 0
Q1 = $(V:1=)
Q = $(Q1:0=@)
ECHO1 = $(V:1=@:)
ECHO = $(ECHO1:0=@echo)

#### Start of system configuration section. ####

srcdir = .
topdir = /Users/ryansobol/.rubies/ruby-2.0.0-p353/include/ruby-2.0.0
hdrdir = $(topdir)
arch_hdrdir = /Users/ryansobol/.rubies/ruby-2.0.0-p353/include/ruby-2.0.0/x86_64-darwin13.0.0
PATH_SEPARATOR = :
VPATH = $(srcdir):$(arch_hdrdir)/ruby:$(hdrdir)/ruby
prefix = /Users/ryansobol/.rubies/ruby-2.0.0-p353
rubysitearchprefix = $(rubylibprefix)/$(sitearch)
rubyarchprefix = $(rubylibprefix)/$(arch)
